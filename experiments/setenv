#
# Environment variables needed to run checked coverage workflow
#

# Java 1.7 and 1.8 path
# checked coverage toolchain requires both
export JDK_1_7=~/jdk1.7.0_80 
export JDK_1_8=~/jdk1.8.0_311 

#
# You probably do not need to modify below
#

# path of top level directory for HCC tools
export HCC_HOME=$PWD/..

# path for experiments directory
export HCC_EXPERIMENTS=$PWD

# path for support libraries and utilities
export HCC_LIBS=$HCC_HOME/lib

# default path of Maven repository
export MAVEN_REPO=~/.m2/repository

# default timeout when tracing
export HCC_TRACE_TIMEOUT_SEC=600
